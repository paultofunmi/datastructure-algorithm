public class MergeSort {

    public static void main(String[] args){

    }

    public static void mergeSort(int[] arr, start, end) {

        if(start < end) {

            int mid = (start + end ) / 2;

            mergeSort(arr, start, mid);
            mergeSort(arr, mid + 1, end);
            
        }
    }
}